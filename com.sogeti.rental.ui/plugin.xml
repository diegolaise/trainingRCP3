<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="palette" name="%extension-point.name" schema="schema/palette.exsd"/>

   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.sogeti.rental.ui.category2"
            name="%category.name">
      </category>
      <view
            category="com.sogeti.rental.ui.category2"
            class="com.sogeti.rental.ui.views.RentalPropertyView"
            icon="icons/RentalObjects.png"
            id="com.sogeti.rental.ui.viewRental"
            name="%view.name"
            restorable="true">
      </view>
      <view
            category="com.sogeti.rental.ui.category2"
            class="com.sogeti.rental.ui.views.AgencyView"
            icon="icons/Agency.png"
            id="com.sogeti.rental.ui.AgencyView"
            name="%view.name.0"
            restorable="true">
      </view>
   </extension>

   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.sogeti.rental.ui.perspective.RentalPerspective"
            icon="icons/Agency.png"
            id="com.sogeti.rental.ui.perspective.RentalPerspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category=""
            class="com.sogeti.rental.ui.pref.RentalPreferencePage"
            id="com.sogeti.rental.ui.preferences"
            name="%page.name">
      </page>
      <page
            category="com.sogeti.rental.ui.palette"
            class="com.sogeti.rental.ui.pref.RentalColorPreferencePage"
            id="com.sogeti.rental.ui.colorprefpage"
            name="%page.name.0">
      </page>
      <page
            category="com.sogeti.rental.ui.preferences"
            class="com.sogeti.rental.ui.pref.RentalColorPreferences"
            id="com.sogeti.rental.ui.palette"
            name="%page.name.1">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sogeti.rental.ui.pref.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.sogeti.rental.ui.handler.HelloHandler"
            commandId="com.sogeti.rental.ui.command.hello">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               commandId="com.sogeti.rental.ui.command.hello"
               id="com.sogeti.rental.ui.textmenu"
               label="%menu.label">
            <command
                  commandId="com.sogeti.rental.ui.command.hello"
                  style="push"
                  tooltip="%command.tooltip">
            </command>
          
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <menu
               id="com.sogeti.rental.ui.textmenu3"
               label="%menu.label.0">
            <command
                  commandId="com.sogeti.rental.ui.command.hello"
                  style="push"
                  tooltip="%command.tooltip.0">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.sogeti.rental.ui.AgencyView?after=additions">
         <menu
               id="com.sogeti.rental.ui.textmenu2"
               label="%menu.label.1">
            <command
                  commandId="com.sogeti.rental.ui.command.hello"
                  style="push"
                  tooltip="%command.tooltip.1">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.sogeti.rental.ui.AgencyView?after=additions">
         <command
               commandId="com.sogeti.rental.ui.command.hello"
               label="%command.label"
               style="push"
               tooltip="%command.tooltip.2">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.sogeti.rental.ui.command.hello"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="com.sogeti.rental.ui.palette">
      <palette
            id="com.sogeti.rental.ui.palette.default"
            name="%palette.name"
            paletteClass="com.sogeti.rental.ui.palette.DefaultPalette">
      </palette>
      <palette
            id="com.sogeti.rental.ui.palette.daltonien"
            name="%palette.name.0"
            paletteClass="com.sogeti.rental.ui.palette.DaltonienPalette">
      </palette>
   </extension>

</plugin>
